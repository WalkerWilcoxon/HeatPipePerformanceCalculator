<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="Field">
        <attr name="name" format="string" />
        <attr name="symbol" format="string" />
    </declare-styleable>
    <declare-styleable name="NumberField">
        <attr name="units" format="string"/>
        <attr name="staticUnits" format="boolean"/>
    </declare-styleable>
    <declare-styleable name="InputNumberText">
        <attr name="initialValue" format="float"/>
        <attr name="minValue" format="float"/>
        <attr name="maxValue" format="float"/>
    </declare-styleable>
    <declare-styleable name="InputEnumeration">
        <attr name="items" format="string"/>
        <attr name="initialValue" format="string"/>
    </declare-styleable>
    <declare-styleable name="OutputNumberText">
        <attr name="equation" format="string"/>
        <attr name="important" format="boolean"/>
    </declare-styleable>
</resources>